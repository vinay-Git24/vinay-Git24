import java.util.Scanner;
import java.io.*;
public class Main
{
    public static void main(String[]args)
    {
        char c[] = {'w','e','l','c','o','m','e',' ','t','o','j','a','v','a',' ','p','r','o','g','r','a','m','m','i','n','g'}; //array of characters
        String str1 = new String(c); //String class object if str1, here we are converting array of characters into String
        System.out.println(str1); //output of str1
    }
}
