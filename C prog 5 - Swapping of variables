/*WAP to swap the values of 2 variables*/
#include <stdio.h>

int main()
{
    int x,y,*a,*b,temp;
    printf("enter the vales of x and y\n");
    scanf("%d%d",&x,&y);
    printf("before swapping\nx=%d\ny=%d\n",x,y);
    a=&x;
    b=&y;
    temp=*b;
    *a=*b;
    *a=temp;
    printf("after swapping\nx=%d\ny=%d\n",x,y);

    return 0;
}
